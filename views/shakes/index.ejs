<%- include('../partials/header') %>

<div style="text-align:center; max-width: 600px;"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSRI3rN5Wczpxayk6p2tnyromvtaahO8Nvppg&usqp=CAU" alt="Shakes" width="100%"></div>


    <table id="list">
        <thead>
            <tr>
            <th>product</th>
            <th>flavor</th>
            <th>calories</th>
            <th>price</th>
            <th>quality</th>
            </tr>
        </thead>
        <tbody>
            <% shakes.forEach(function(s) { %>
                <tr>
                    <td><%=s.product%></td>
                    <td><%=s.flavor %></td>
                    <td><%=s.calories %></td>
                    <td><%=s.price %></td>
                    <td><%=s.quality %></td>
                    <td><a href="/shakes/<%= s._id %>">DETAILS</a></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
 <%- include('../partials/footer') %>