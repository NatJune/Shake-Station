<%- include('../partials/header') %>
<table id="list">
        <thead>
            <tr>
            <th>name:</th>
            <th>email:</th>
            <th>avatarURL:</th>
            <th>googleId</th>
            </tr>
        </thead>
        <tbody>
            <tbody>
                <% shake.user.forEach(user => { %>
                  <tr>
                    <td><a href="/user/<%=user_.id%>">User Login</a></td>
                    <td><%= user.name%></td>
                    <td><%= user.email %></td>
                    <td><%= user.avatarURL %></td>
                    <td><%= user.googleId %></td>
                    <td><form action="/user/<%= user._id %>?_method=DELETE" method="POST">  
                    <input type="submit" value="delete"></input>
                    </form>
                  </td>
                  </tr>
                <% }) %>
              </tbody>
        </tbody>
      </table>


<%- include('../partials/footer') %>